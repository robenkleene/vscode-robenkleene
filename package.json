{
  "name": "robenkleene",
  "displayName": "RobenKleene",
  "publisher": "RobenKleene",
  "description": "Roben Kleene's personal customizations.",
  "repository": "https://github.com/robenkleene/vscode-robenkleene/",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.openURLs",
    "onCommand:extension.openURLsInIINA",
    "onCommand:extension.archive",
    "onCommand:extension.openScratchFile",
    "onCommand:extension.openScratchFileInSplit",
    "onCommand:extension.convertToSlug",
    "onCommand:extension.blogPostFromFile",
    "onCommand:extension.blogLinkFromFile",
    "onCommand:extension.openNotes",
    "onCommand:extension.openSocial",
    "onCommand:extension.newEmail",
    "onCommand:extension.saveAsInbox",
    "onCommand:extension.makeWikiLink",
    "onCommand:extension.makeTitleCase",
    "onCommand:extension.todoCheck",
    "onCommand:extension.todoUncheck",
    "onCommand:extension.todoToggle",
    "onCommand:extension.todoToggleLocal",
    "onCommand:extension.openDirectory",
    "onCommand:extension.closeOtherEditors",
    "onCommand:extension.quickOpenZ",
    "onCommand:extension.slugProject",
    "onCommand:extension.slugProjectArchive",
    "onCommand:extension.slugProjectArchiveReadme",
    "onCommand:extension.insertTitle",
    "onCommand:extension.openSourceControlSite",
    "onCommand:extension.insertFilePath",
    "onCommand:extension.quickOpenDirectory",
    "onCommand:extension.copyMarkdownSourceControlLink",
    "onCommand:extension.copyMarkdownSourceControlQuote",
    "onCommand:extension.openSourceControlLink",
    "onCommand:extension.quickOpenDocumentation",
    "onCommand:extension.quickOpenText",
    "onCommand:extension.quickOpenAll",
    "onCommand:extension.quickOpenDeveloper",
    "onCommand:extension.quickOpenAllFiles",
    "onCommand:extension.openDocumentation",
    "onCommand:extension.openTweets",
    "onCommand:extension.newInboxDocument",
    "onCommand:extension.openJournalEntry",
    "onCommand:extension.openDevelopmentScratch",
    "onCommand:extension.openBlog",
    "onCommand:extension.openWiki",
    "onCommand:extension.openProjects",
    "onCommand:extension.openPersonalWiki",
    "onCommand:extension.openInRepla",
    "onCommand:extension.openReadme",
    "onCommand:extension.openFile"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.openURLs",
        "title": "Open URLs"
      },
      {
        "command": "extension.openURLsInIINA",
        "title": "Open URLs in IINA"
      },
      {
        "command": "extension.makeWikiLink",
        "title": "Make Wiki Link"
      },
      {
        "command": "extension.archive",
        "title": "Archive"
      },
      {
        "command": "extension.openScratchFile",
        "title": "Open Scratch File"
      },
      {
        "command": "extension.openScratchFileInSplit",
        "title": "Open Scratch File in Split"
      },
      {
        "command": "extension.convertToSlug",
        "title": "Convert to Slug"
      },
      {
        "command": "extension.slugProject",
        "title": "Slug Project: New"
      },
      {
        "command": "extension.slugProjectArchive",
        "title": "Slug Project: Archive"
      },
      {
        "command": "extension.slugProjectArchiveReadme",
        "title": "Slug Project: Open Archive README"
      },
      {
        "command": "extension.quickOpenDocumentation",
        "title": "Quick Open Documentation"
      },
      {
        "command": "extension.quickOpenText",
        "title": "Quick Open Text"
      },
      {
        "command": "extension.quickOpenAll",
        "title": "Quick Open All"
      },
      {
        "command": "extension.quickOpenAllFiles",
        "title": "Quick Open All Files"
      },
      {
        "command": "extension.quickOpenDeveloper",
        "title": "Quick Open Developer"
      },
      {
        "command": "extension.openDocumentation",
        "title": "Open Documentation"
      },
      {
        "command": "extension.openTweets",
        "title": "Open Tweets"
      },
      {
        "command": "extension.openWiki",
        "title": "Open Wiki"
      },
      {
        "command": "extension.openProjects",
        "title": "Open Projects"
      },
      {
        "command": "extension.openPersonalWiki",
        "title": "Open Personal Wiki"
      },
      {
        "command": "extension.openDevelopmentScratch",
        "title": "Open Developer Scratch"
      },
      {
        "command": "extension.openBlog",
        "title": "Open Blog"
      },
      {
        "command": "extension.blogPostFromFile",
        "title": "Blog Post From File"
      },
      {
        "command": "extension.blogLinkFromFile",
        "title": "Blog Link From File"
      },
      {
        "command": "extension.openNotes",
        "title": "Open Notes"
      },
      {
        "command": "extension.openSocial",
        "title": "Open Social"
      },
      {
        "command": "extension.newEmail",
        "title": "New Email"
      },
      {
        "command": "extension.newInboxDocument",
        "title": "New Inbox Document"
      },
      {
        "command": "extension.openJournalEntry",
        "title": "Open Journal Entry"
      },
      {
        "command": "extension.openDirectory",
        "title": "Open Directory"
      },
      {
        "command": "extension.closeOtherEditors",
        "title": "Close Other Editors"
      },
      {
        "command": "extension.quickOpenZ",
        "title": "Quick Open Z"
      },
      {
        "command": "extension.saveAsInbox",
        "title": "Save As Inbox"
      },
      {
        "command": "extension.insertFilePath",
        "title": "Insert File Path"
      },
      {
        "command": "extension.quickOpenDirectory",
        "title": "Quick Open Directory"
      },
      {
        "command": "extension.insertTitle",
        "title": "Insert Title"
      },
      {
        "command": "extension.copyMarkdownSourceControlLink",
        "title": "Copy Markdown Source Control Link"
      },
      {
        "command": "extension.copyMarkdownSourceControlQuote",
        "title": "Copy Markdown Source Control Quote"
      },
      {
        "command": "extension.openSourceControlLink",
        "title": "Open Source Control Link"
      },
      {
        "command": "extension.openSourceControlSite",
        "title": "Source Control: Open Site"
      },
      {
        "command": "extension.makeTitleCase",
        "title": "Make Title Case"
      },
      {
        "command": "extension.todoCheck",
        "title": "Todo: Check"
      },
      {
        "command": "extension.todoUncheck",
        "title": "Todo: Uncheck"
      },
      {
        "command": "extension.todoToggle",
        "title": "Todo: Toggle"
      },
      {
        "command": "extension.todoToggleLocal",
        "title": "Todo: Toggle Local"
      },
      {
        "command": "extension.openInRepla",
        "title": "Open in Repla"
      },
      {
        "command": "extension.openReadme",
        "title": "Open README"
      },
      {
        "command": "extension.openFile",
        "title": "Open File"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.archive"
        },
        {
          "command": "extension.openScratchFile"
        },
        {
          "command": "extension.openScratchFileInSplit"
        },
        {
          "command": "extension.makeWikiLink",
          "when": "resourceLangId == markdown"
        },
        {
          "command": "extension.todoCheck",
          "when": "resourceLangId == markdown"
        },
        {
          "command": "extension.todoUncheck",
          "when": "resourceLangId == markdown"
        },
        {
          "command": "extension.todoToggle",
          "when": "resourceLangId == markdown"
        },
        {
          "command": "extension.slugProject",
          "when": "resourceLangId == markdown"
        },
        {
          "command": "extension.openURLs"
        },
        {
          "command": "extension.openURLsInIINA"
        },
        {
          "command": "extension.makeTitleCase"
        }
      ],
      "explorer/context": [
        {
          "command": "extension.archive",
          "group": "navigation"
        },
        {
          "command": "extension.openReadme",
          "when": "explorerResourceIsFolder",
          "group": "navigation"
        },
        {
          "command": "extension.blogPostFromFile",
          "when": "resourceLangId == markdown",
          "group": "navigation"
        },
        {
          "command": "extension.blogLinkFromFile",
          "when": "resourceLangId == markdown",
          "group": "navigation"
        },
        {
          "command": "extension.openSourceControlSite",
          "when": "explorerResourceIsFolder",
          "group": "navigation"
        },
        {
          "command": "extension.openInRepla",
          "group": "navigation"
        },
        {
          "command": "extension.slugProject",
          "when": "explorerResourceIsFolder",
          "group": "navigation"
        },
        {
          "command": "extension.slugProjectArchive",
          "when": "explorerResourceIsFolder",
          "group": "navigation"
        },
        {
          "command": "extension.slugProjectArchiveReadme",
          "when": "explorerResourceIsFolder",
          "group": "navigation"
        },
        {
          "command": "extension.closeOtherEditors",
          "when": "!explorerResourceIsFolder",
          "group": "navigation"
        },
        {
          "command": "extension.openDirectory",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "extension.openURLs",
        "key": "alt+l",
        "when": "!terminalFocus && editorHasSelection"
      },
      {
        "command": "extension.openFile",
        "key": "ctrl+x ctrl+f",
        "when": "!terminalFocus"
      },
      {
        "command": "extension.insertFilePath",
        "key": "alt+s cmd-p",
        "when": "!terminalFocus"
      },
      {
        "command": "extension.insertFilePath",
        "key": "alt+s e",
        "when": "!terminalFocus"
      },
      {
        "command": "extension.quickOpenDirectory",
        "key": "alt+c",
        "when": "!terminalFocus"
      },
      {
        "command": "extension.makeWikiLink",
        "key": "alt+s k",
        "when": "!terminalFocus"
      },
      {
        "command": "extension.makeTitleCase",
        "key": "alt+s c",
        "when": "!terminalFocus"
      },
      {
        "command": "extension.slugProject",
        "key": "alt+s p",
        "when": "!terminalFocus"
      },
      {
        "command": "extension.archive",
        "key": "ctrl+alt+d",
        "when": "!terminalFocus"
      },
      {
        "command": "extension.archive",
        "key": "alt+s d",
        "when": "!terminalFocus"
      },
      {
        "command": "extension.insertTitle",
        "key": "alt+s t",
        "when": "!terminalFocus"
      },
      {
        "command": "extension.closeOtherEditors",
        "key": "alt+s o",
        "when": "!terminalFocus"
      },
      {
        "command": "extension.copyMarkdownSourceControlQuote",
        "key": "alt+m",
        "when": "!terminalFocus"
      },
      {
        "command": "extension.quickOpenZ",
        "key": "alt+z",
        "when": "!terminalFocus"
      },
      {
        "command": "extension.todoToggleLocal",
        "key": "alt+enter",
        "when": "!terminalFocus && !findWidgetVisible && resourceLangId == markdown"
      },
      {
        "command": "extension.quickOpenAllFiles",
        "key": "alt+shift+o",
        "when": "!terminalFocus"
      },
      {
        "command": "extension.quickOpenDeveloper",
        "key": "alt+g",
        "when": "!terminalFocus"
      },
      {
        "command": "extension.quickOpenAll",
        "key": "alt+o",
        "when": "!terminalFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.42.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "typescript": "^3.7.5",
    "vscode-test": "^1.3.0"
  }
}
